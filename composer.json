{
    "name": "axazara/moneroo-laravel",
    "description": "This is simple PHP Package for Laravel to interact with Moneroo API.",
    "keywords": [
        "AxaZara",
        "laravel",
        "moneroo",
        "moneroo.io",
        "moneroo-laravel"
    ],
    "homepage": "https://docs.moneroo.io/",
    "license": "MIT",
    "authors": [
        {
            "name": "Elias Elimah",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.7",
        "illuminate/contracts": "^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.20",
        "insolita/unused-scanner": "^2.4",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.6",
        "orchestra/testbench": "^7.24",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9.6",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Moneroo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moneroo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "sniff": "vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots",
        "format": "vendor/bin/php-cs-fixer fix",
        "unused": "vendor/bin/unused_scanner unused-scanner.php",
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phpunit --testdox"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moneroo\\Providers\\MonerooServiceProvider"
            ],
            "aliases": {
                "MonerooPayment": "Moneroo\\Facades\\PaymentFacade",
                "MonerooPayout": "Moneroo\\Facades\\PayoutFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
